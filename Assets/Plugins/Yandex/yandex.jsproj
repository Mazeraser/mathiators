var player;

ysdk.getPlayer({scopes: false;}).then(_player => {
        player = _player;
    }).catch(err => {
        // Ошибка при инициализации объекта Player.
    });

function SaveData(data){
    player.setData({
        score: data,
    },false)
}
function LoadData(){
    player.getData().then(_data => myGameInstance.SendMessage("MenuService","SetScore",_data));
}